  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#7D8087",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#d2a6ff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#ffad66"
      }
    },
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#ffad66"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#51bacf"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#e6bc68",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#c46d61"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#6D7178"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#d2a6ff"
      }
    },
    {
